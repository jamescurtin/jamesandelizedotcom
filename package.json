  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "gatsby": "^3.14.5",
    "gatsby-plugin-google-gtag": "^3.14.0",
    "gatsby-plugin-image": "^1.14.2",
